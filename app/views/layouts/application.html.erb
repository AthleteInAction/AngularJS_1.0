<!DOCTYPE html>
<html ng:app="App" ng:controller="Ctrl">
<head>
  <title><%= TITLE %></title>
  <%= stylesheet_link_tag    'application' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

	<% flash.each do |type,message| %>
		<alert type="<%= type %>"><%= message %></alert>
	<% end %>

	<%= yield %>

</body>
</html>